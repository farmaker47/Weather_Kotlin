<?xml version="1.0" encoding="utf-8"?>
<navigation xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/nav_graph"
    app:startDestination="@id/WeatherFragment">

    <fragment
        android:id="@+id/WeatherFragment"
        android:name="com.george.weather_kotlin.weather_fragment.WeatherFragment"
        android:label="@string/weather_fragment_label"
        tools:layout="@layout/fragment_first">
        <argument android:name="latitude"
            app:argType="string"/>
        <argument android:name="longtitude"
            app:argType="string"/>
        <argument android:name="address"
            app:argType="string"/>
    </fragment>
</navigation>
